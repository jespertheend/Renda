		"@webgpu/types": "^0.1.6",
		"babel-eslint": "^10.1.0",
		"concurrently": "5.x",
		"eslint": "^7.32.0",
		"google-closure-compiler": "^20210505.0.0",
		"js-md5": "^0.7.3",
		"rollup": "^2.47.0",
