{
	"rules": {
		// Sometimes this is necessary for closure compiler
		"quote-props": "off",

		// Closure Compiler needs quotes to access named capture groups
		// So using regular capture groups is preferred
		"prefer-named-capture-group": "off",

		// Required for DEBUG_INCLUDE_ERROR_MESSAGES
		"no-throw-literal": "off",

		// Closure Compiler doesn't support `this` in static methods.
		"rulesdir/no-this-in-static-method": "error"
	}
}
